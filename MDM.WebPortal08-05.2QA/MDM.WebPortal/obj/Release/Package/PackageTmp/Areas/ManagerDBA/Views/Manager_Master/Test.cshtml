@using System.Collections.Generic
@using Kendo.Mvc.UI
@model IEnumerable<MDM.WebPortal.Areas.ManagerDBA.Models.ViewModels.VMManager_Master>

@{
    ViewBag.Title = "Test";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}

<h2>Test</h2>
<div class="container">
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        @(Html.Kendo().AutoComplete()
          .Name("ManagerID") //The name of the AutoComplete is mandatory. It specifies the "id" attribute of the widget.
          .DataTextField("AliasName") //Specify which property of the Product to be used by the AutoComplete.
          .BindTo(Model)   //Pass the list of Products to the AutoComplete.
          .Filter("contains") //Define the type of the filter, which AutoComplete will use.
              )
        
        <button type="submit">Send</button>
    }
</div>
