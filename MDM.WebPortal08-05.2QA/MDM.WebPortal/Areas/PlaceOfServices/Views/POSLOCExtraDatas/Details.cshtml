@model MDM.WebPortal.Models.FromDB.POSLOCExtraData

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}

<header class="main-header">
    <div class="container">
        <h1 class="page-title"></h1>
        <ol class="breadcrumb pull-right">
            <li><a href="@Url.Action("Index", "Home", new {area = ""})"><i class="fa fa-home"></i> Home</a></li>
            <li><a href="@Url.Action("Index", "LocationsPOS", new {area = "PlaceOfServices"})"> LocationsPOS</a></li>
            <li class="active">Info</li>
        </ol>
    </div>
</header>

<div class="container-fluid">
    <div class="row">
    <div class="col-md-offset-2 col-md-8">
        <ul class="nav nav-pills nav-justified ar-nav-pills center-block margin-bottom">
            <li class="active"><a href="#freetext" data-toggle="tab" aria-expanded="true"><i class="fa fa-clone"></i> Info</a></li>
            <li class=""><a href="#booleans" data-toggle="tab" aria-expanded="false"><i class="fa fa-question-circle"></i> Questions</a></li>
        </ul>
        <div class="tab-content margin-top">
            <div class="tab-pane active" id="freetext">

                <div class="pricign-box pricign-box-pro">
                    <div class="pricing-box-header">
                        <h2></h2>
                        
                    </div>
                    <div class="pricing-box-price">
                        <h3> </h3>
                    </div>
                    <div class="pricing-box-content">
                        <ul>
                            <li><i class="fa fa-phone-square" style="color: #0099da"></i> <strong class="text-uppercase">@Html.DisplayNameFor(model => model.Phone_Number):</strong> @Html.DisplayFor(model => model.Phone_Number).</li>
                            <li><i class="fa fa-fax" style="color: #0099da"></i> <strong class="text-uppercase">@Html.DisplayNameFor(model => model.Fax_Number):</strong> @Html.DisplayFor(model => model.Fax_Number).</li>
                            <li><i class="fa fa-globe" style="color: #0099da"></i> <strong class="text-uppercase">@Html.DisplayNameFor(model => model.Website):</strong> @Html.DisplayFor(model => model.Website).</li>
                            <li><i class="fa fa-bed" style="color: #0099da"></i> <strong class="text-uppercase">@Html.DisplayNameFor(model => model.Number_of_beds):</strong> @Html.DisplayFor(model => model.Number_of_beds).</li>
                            <li><i class="fa fa-calendar" style="color: #0099da"></i> <strong class="text-uppercase">@Html.DisplayNameFor(model => model.how_many_days_week_open):</strong> @Html.DisplayFor(model => model.how_many_days_week_open).</li>
                            <li><i class="fa fa-building" style="color: #0099da"></i> <strong class="text-uppercase">@Html.DisplayNameFor(model => model.Lab_Name):</strong> @Html.DisplayFor(model => model.Lab_Name).</li>
                            <li><i class="fa fa-hashtag" style="color: #0099da"></i> <strong class="text-uppercase">@Html.DisplayNameFor(model => model.BCBS_ID_Number):</strong> @Html.DisplayFor(model => model.BCBS_ID_Number).</li>
                            <li><i class="fa fa-hashtag" style="color: #0099da"></i> <strong class="text-uppercase">@Html.DisplayNameFor(model => model.UPIN_Number):</strong> @Html.DisplayFor(model => model.UPIN_Number).</li>
                            <li><i class="fa fa-hashtag" style="color: #0099da"></i> <strong class="text-uppercase">@Html.DisplayNameFor(model => model.Medicaid_Number):</strong> @Html.DisplayFor(model => model.Medicaid_Number).</li>
                            <li><i class="fa fa-map-marker" style="color: #0099da"></i> <strong class="text-uppercase">@Html.DisplayNameFor(model => model.State_of_MD_or_PhyGrp):</strong> @Html.DisplayFor(model => model.State_of_MD_or_PhyGrp).</li>
                            <li><i class="fa fa-info-circle" style="color: #0099da"></i> <strong class="text-uppercase">@Html.DisplayNameFor(model => model.JACHO_CARF):</strong> @Html.DisplayFor(model => model.JACHO_CARF).</li>
                            <li><i class="fa fa-user" style="color: #0099da"></i> <strong class="text-uppercase">@Html.DisplayNameFor(model => model.Manage_care_contracts):</strong> @Html.DisplayFor(model => model.Manage_care_contracts).</li>
                            <li><i class="fa fa-calendar-check-o" style="color: #0099da"></i> <strong class="text-uppercase">@Html.DisplayNameFor(model => model.In_Service_date_time):</strong> @Html.DisplayFor(model => model.In_Service_date_time).</li>
                            <li><i class="fa fa-paper-plane" style="color: #0099da"></i> <strong class="text-uppercase">forms sent:</strong> @ViewBag.FormSent</li>
                        </ul>
                    </div>
                    <div class="pricing-box-footer">
                        
                    </div>
                </div>

               
            </div>

            <div class="tab-pane" id="booleans">
                <dl class="dl-horizontal">
                    <dt>
                        @Html.DisplayNameFor(model => model.Portal_training_setup)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Portal_training_setup)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.email_regarding_conference_set_up)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.email_regarding_conference_set_up)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Database_set_up)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Database_set_up)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Availavility_request_sent_out)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Availavility_request_sent_out)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Availavility_completed)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Availavility_completed)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Navinet_request_completed)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Navinet_request_completed)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Fee_schedule_in_binder)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Fee_schedule_in_binder)
                    </dd>

                </dl>
            </div>

        </div>

    </div>

    <div class="col-md-offset-2 col-md-8">
        @if (Model.FACInfoData_FACInfoDataID == 0)
        {
            <div class="pull-right">
                <a href="@Url.Action("Create", new { facInfoData = ViewBag.FACInfoDataID })" class="btn btn-ar btn-primary">
                    <i class="fa fa-cloud-upload"></i> FILL OUT
                </a>
                <a href="@Url.Action("Index", "LocationsPOS", new {area = "PlaceOfServices"})" class="btn btn-ar btn-danger">
                    <i class="fa fa-ban"></i> CANCEL
                </a>
            </div>
        }
        else
        {
            <div class="pull-right">
                <a href="@Url.Action("Edit", new { id = Model.FACInfoData_FACInfoDataID })" class="btn btn-ar btn-primary">
                    <i class="fa fa-cloud-upload"></i> UPDATE
                </a>
                <a href="@Url.Action("Index", "LocationsPOS", new {area = "PlaceOfServices"})" class="btn btn-ar btn-danger">
                    <i class="fa fa-ban"></i> CANCEL
                </a>
            </div>
        }

    </div>
   
    </div>
</div>

