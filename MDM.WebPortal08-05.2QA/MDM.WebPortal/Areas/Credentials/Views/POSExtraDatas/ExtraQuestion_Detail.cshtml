}

<div class="container-fluid">
    <a class="k-button k-button-icontext k-add-button" href="#"><span class="k-icon k-add"></span>NEW</a>
    <br/>
    <br/>
    @(Html.Kendo().ListView<MDM.WebPortal.Areas.Credentials.Models.ViewModel.VMPOSLOCExtraData>(Model)
          .Name("lvQuestion_" + pos)
          .TagName("div")
          .ClientTemplateId("lvQuestion_template")
          .DataSource(dataSource => dataSource
              .Model(model => model.Id("POSExtraDataID"))
              .PageSize(1)
              .Create(create => create.Action("Create_ExtraData", "POSExtraDatas", new {ParentID = pos}))
              .Update(update => update.Action("Update_ExtraData", "POSExtraDatas"))
          )
          .Editable(ed => ed.TemplateName("POSExtraQVMEditor"))
